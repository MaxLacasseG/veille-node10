<% include partials/head %>

    <body>
        <div id="conteneurPrinc">
            <header>
                <div id="groupeEntete">
                    <h1>TRI</h1>
                    <h2>Liste des membres</h2>
                </div>
                <% include partials/nav %>
            </header>
            <main>
                <section id="principale">
                    <table>
                        <thead>
                            <tr>
                                Les membres
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>Prénom</td>
                                <td>Nom</td>
                                <td>Téléphone</td>
                                <td>Courriel</td>
                            </tr>
                            <% for(let elem of data){ %>

                                <tr>
                                    <td>
                                        <%= elem.prenom %>
                                    </td>
                                    <td>
                                        <%= elem.nom %>
                                    </td>
                                    <td>
                                        <%= elem.tel %>
                                    </td>
                                    <td>
                                        <%= elem.courriel %>
                                    </td>
                                </tr>
                                <% }; %>
                        </tbody>
                    </table>
                </section>
            </main>
            <% include partials/footer %>
        </div>
    </body>

    </html>